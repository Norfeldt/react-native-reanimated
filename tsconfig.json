{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "react-native-reanimated": ["./src/Animated"]
    },
    "declaration": true,
    "preserveSymlinks": true,
    "target": "es6",
    "module": "es6",
    "jsx": "react-native",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "lib": ["es6", "dom"],
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
  }
}
